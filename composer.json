{
  "name": "torann/elasticquent",
  "type": "library",
  "description": "Maps Laravel Eloquent models to Elasticsearch types.",
  "keywords": [
    "elasticsearch",
    "eloquent",
    "laravel"
  ],
  "homepage": "https://github.com/Torann/Elasticquent",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "daniel@lyften.com",
      "homepage": "http://lyften.com"
    },
    {
      "name": "Adam Fairholm",
      "email": "adam.fairholm@gmail.com",
      "homepage": "http://adamfairholm.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "illuminate/support": "5.2 - 5.3",
    "illuminate/database": "5.2 - 5.3",
    "illuminate/config": "5.2 - 5.3",
    "elasticsearch/elasticsearch": ">2.0 <2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.2|~5.0",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Elasticquent\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
